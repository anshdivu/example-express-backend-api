    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/morgan": "^1.7.35",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
